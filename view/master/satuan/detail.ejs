<h4 class="text-capitalize">Detail Jenis Barang</h4>

<div class="row">
    <div class="col-lg-6">
        <div class="card">
            <div class="card-body">
                <h5 class="card-title"><%= satuan[0].nama_satuan %></h5>
                <p class="card-text">
                    Kode: <%= satuan[0].kode %> <br>
                    Description: <%= satuan[0].description %> <br>
                    waktu pembuatan: <%= moment(satuan[0].created_at).format('DD MMMM YYYY HH:mm') %> <br>
                    dibuat oleh: <%= satuan[0].created_nama %>
                </p>
            </div>
        </div>
    </div>
</div>